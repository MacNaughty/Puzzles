total_english_subscribers = int(input())
english_newspaper_roll_numbers = set()
english_newspaper_roll_numbers.update(map(int, input().split(' ')))

total_french_subscribers = int(input())
french_newspaper_roll_numbers = set()
french_newspaper_roll_numbers.update(map(int, input().split(' ')))

def unique_subcribers(english_newspaper_roll_numbers, french_newspaper_roll_numbers):
    all_subscribers = english_newspaper_roll_numbers.union(french_newspaper_roll_numbers)
    print(len(all_subscribers))

unique_subcribers(english_newspaper_roll_numbers, french_newspaper_roll_numbers)
