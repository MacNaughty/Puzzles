total_english_subscribers = int(input())
english_newspaper_roll_numbers = set()
english_newspaper_roll_numbers.update(map(int, input().split(' ')))

total_french_subscribers = int(input())
french_newspaper_roll_numbers = set()
french_newspaper_roll_numbers.update(map(int, input().split(' ')))

def subcribed_to_both(english_newspaper_roll_numbers, french_newspaper_roll_numbers):
    subscribed_to_both = english_newspaper_roll_numbers.intersection(french_newspaper_roll_numbers)
    print(len(subscribed_to_both))

subcribed_to_both(english_newspaper_roll_numbers, french_newspaper_roll_numbers)
